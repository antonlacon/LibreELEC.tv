echo "OLD_PROJECT_ARCH=${OLD_PROJECT_ARCH}"
echo "NEW_PROJECT_ARCH=${NEW_PROJECT_ARCH}"
echo "OLD_DISTRO_VERSION=${OLD_DISTRO_VERSION}"
